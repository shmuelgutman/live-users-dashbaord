{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base", "prettier", "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "prettier/prettier": ["error"],
        "consistent-return": "off",
        "no-console": "off"
    },
    "plugins": ["jest", "prettier"]
}
